<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="300.0" minWidth="400.0" prefHeight="300.0" prefWidth="400.0" spacing="30.0"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.mck.controller.GameOverController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Congratulations!" textAlignment="CENTER">
        <font>
            <Font size="36.0" />
        </font>
    </Text>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="You solved the Sudoku!" textAlignment="CENTER">
        <font>
            <Font size="20.0" />
        </font>
    </Text>
    <Button fx:id="startNewGameButton" onAction="#startNewGameButtonClicked" text="Start New Game" />
</VBox>
